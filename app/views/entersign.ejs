<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Enter vital signs</title>
    <link rel="stylesheet" href="css/feedback.css" />
  </head>
  <body>
    <div id="allpage">
      <h1>Enter vital signs Form</h1>
      <div id="form">
        <div id="line"></div>
        <form action="/signs" method="post">
          <div class="form-group">
            <label for="pulseRate">Pulse Rate:</label>
            <input type="text" name="pulseRate" required />
          </div>
          <div class="form-group">
            <label for="bloodPressure">BloodPressure:</label>
            <input type="text" name="bloodPressure" required />
          </div>
          <div class="form-group">
            <label for="weight">Weight:</label>
            <input type="text" name="weight" required />
          </div>
          <div class="form-group">
            <label for="temperature">Temperature:</label>
            <input type="text" name="temperature" required />
          </div>
          <div class="form-group">
            <label for="respiratory">Respiratory:</label>
            <input type="text" name="respiratory" required />
          </div>
          <br />
          <button type="submit">Submit</button>
        </form>
      </div>
    </div>
  </body>
</html>
